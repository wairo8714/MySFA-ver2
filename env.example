# MySFA 環境変数設定ファイルの例
# このファイルをコピーして .env として使用してください

# Django設定
SECRET_KEY=your-django-secret-key-here
DEBUG=False
ALLOWED_HOSTS=mysfa.net,*.mysfa.net,localhost,127.0.0.1

# MySQL設定
MYSQL_HOST=db
MYSQL_PORT=3306
MYSQL_DATABASE=mysfa_db
MYSQL_USER=admin
MYSQL_PASSWORD=your-secure-password
MYSQL_ROOT_PASSWORD=your-root-password

# 本番環境設定
FORCE_HTTPS=True
